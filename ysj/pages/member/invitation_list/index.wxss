/* pages/member/invitation_list/index.wxss */
page {
    background-color: #f3f3f3;
    font-size: 16px;
    height: 100%;
}
.main{
  background: #fff;
}
.list{
    position: relative;
    display: flex;
    padding: 16rpx 24rpx;
    transition-duration: 300ms;
    align-items: center;
    overflow: hidden;
}

.listImg {
    display: flex;
    flex-shrink: 0;
    flex-wrap: nowrap;
    box-sizing: border-box;
    align-items: center;
    margin-right: 20rpx;
    color: #000;
    position: relative;
}
.listImg image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}
.listName {
    position: relative;
    flex: 1;
    overflow: hidden;
    box-sizing: border-box;
    display: block;
}
.view_box{
  /* display: flex; */
  width: 100%;
  height: 100%;
  /* align-items: center; */
  /* justify-content: center; */
  margin: 200rpx auto 0;
  font-size: 32rpx;
  text-align: center;
  color: #666;
  background: transparent;
}
.view_box image{
  width: 250rpx;
}