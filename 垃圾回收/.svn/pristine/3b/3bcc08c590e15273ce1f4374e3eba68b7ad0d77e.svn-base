/* pages/classification/index/index.wxss */
/* page{
  background: #fff;
} */
.headIndex{
  width: 95%;
  margin: 0 auto;
  border-radius: 10px;
  overflow: hidden;
}
.contont{
   width: 95%;
   margin: 20rpx auto 0;
}
.headIndex image{
  width: 100%;
  height: auto;
  overflow: hidden;
}
/*用来包裹所有的小圆点  */
.dots{  
  /* width: 156rpx; */
  height: 36rpx; 
  display: flex;
  flex-direction: row;
  position: absolute;
   left: 51%;
    top: 93%;
    transform: translate(-50%, -50%);
}  
/*未选中时的小圆点样式 */
.dot{  
  width: 50rpx;  
  height: 17rpx;   
  margin-right: 26rpx;
  background-color: white;
}  
/*选中以后的小圆点样式  */
.active{  
width: 50rpx;  
  height: 17rpx;  
  background-color: coral;
}  
.images{
  height: auto;
  position: relative; 
}
.images swiper{
  overflow: hidden;
  border-radius: 6px;
}
.images swiper swiper-item{
   overflow: hidden;
  border-radius: 6px;
}
.headClass{
  display: flex;
  flex-flow:  row nowrap;
  justify-content: space-between;
  height: 200rpx;
}
.headClass > .classLeft{
  width: 346rpx;
}
.headClass > .classLeft image{
  width: 100%
}
.headClass > .classright{
  width: 346rpx;
}
.headClass > .classright image{
  width: 100%
}
.M-title{
  width: 100%;
  position: relative;
  height: 55rpx;
}
.M-title> .info{
width: 198rpx;
font-size: 32rpx;
position: absolute;
left: 0;
right: 0;
margin-left: auto;
margin-right: auto;
color: #666
}
.M-title> .more{
font-size: 28rpx;
position: absolute;
right: 10rpx;
width: 115rpx;
height: 40rpx;
background: #97c73f;
border-radius: 20px;
color: #fff;
text-align: center
}
.M-news-list{
  margin-top: 15rpx;
}
.M-news-list > .item{
  width: 100%;
  height: 210rpx;
  padding: 16rpx;
  background: #f5f5f5;
  border-radius: 6px;
  box-sizing: border-box;
  margin-bottom: 30rpx;
}
.M-news-list > .item > .item-list{
  display: flex;
}
 
.M-news-list > .item > .item-list > .item-left{
  width: 195rpx;
  margin-right: 20rpx;
  display: flex;
  align-items: center;
}
.M-news-list > .item > .item-list > .item-right{
  width: 60%;
}
.M-news-list > .item > .item-list > .item-left image{
  width: 100%;
  max-height:180rpx;

}
.M-news-list > .item > .item-list > .item-right > .top{
  height: 130rpx;
  color: #333;
  font-size: 32rpx;
  overflow: hidden;
}
.M-news-list > .item > .item-list > .item-right > .bottom{
  display: flex;
  justify-content: space-between;
  /* height:  */
}
.M-news-list > .item > .item-list > .item-right > .bottom > .day{
  width: 50%;
  color: #7a7a7a;
  font-size: 32rpx;
display: flex;
  align-items: center;
}
.M-news-list > .item > .item-list > .item-right > .bottom > .ico{
  width: 50%;
  display: flex;
  align-items: center;
  justify-content:flex-end;

}
.M-news-list > .item > .item-list > .item-right > .bottom > .ico >.imgs{
  width: 20%;
}
.M-news-list > .item > .item-list > .item-right > .bottom > .ico >.imgs image{
  width: 100%;
}
.M-news-list > .item > .item-list > .item-right > .bottom > .ico >.font{
  color: #7a7a7a;
  font-size: 30rpx;
  margin-left: 10rpx;
  margin-top:10rpx;

}
.navclass{
  width:95%;
  height: 250rpx;
  margin: 30rpx auto;
  display: flex;
}
.navclass > .ico{
  width: 178rpx;
  height: 178rpx;
  /* background: red; */
  margin: 0 auto;
  text-align: center
}
.navclass > .ico >image{
  width: 140rpx;
}
.navclass > .ico > .font{
  font-size: 30rpx;
  text-align: center;
  margin-top: 13rpx;
}
.process{
  text-align: center;
  font-size:32rpx;
color:#666;
margin-top: 20rpx;
}