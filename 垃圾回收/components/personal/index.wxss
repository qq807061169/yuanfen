  width: 76%;
  margin: 0 auto;
  height: 600rpx;
  opacity: 1;
  z-index: 99999;
  position: relative;
  top: 234rpx;
  border-radius: 8px;
}
.Modal-Font{
  position: absolute;
  top: 0rpx;
  color: #4f881c;
  font-size: 40rpx;
  text-align: center;
 margin-top:52rpx;
   left: 0; 
  right: 0; 
  margin-left: auto; 
  margin-right: auto; 
}
.ysj-Modal-wrap .modal-img{
  position: absolute;
 top: 165rpx;
  display: block;
  width: 50%;
    left: 0; 
  right: 0; 
