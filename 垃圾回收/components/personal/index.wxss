/* pages/classification/personal/index.wxss */
page{
  background: #fcfcfc;
}
.contont{
   width: 95%;
   margin: 20rpx auto 0;
}
.head_info{
  height: 258rpx ;
  background: #97c73f;
  border-radius: 6px;
  position: relative;
  display: flex;
}
.head_info > .imgs{
  position: absolute;
  width: 50%;
  left: 20rpx;
  bottom: 0;
  z-index: 999999999;
}
.head_info > .imgs image{
  position: absolute;
  width: 50%;
  left: 20rpx;
  bottom: 0
}
.head_info >.infoLeft{
  display: flex;
  align-items: center;
  width: 50%;
}
.head_info >.infoLeft >.imgs{
  /* width: 50%; */
  width: 160rpx;
  height: 160rpx;
  margin-left: 25rpx;
}
.head_info >.infoLeft >.imgs > image{
  width: 100%;
  border: 2px solid #fff;
  border-radius: 50%;
}
.head_info >.infoLeft >.info-name{
  width: 50%;
  margin-left: 25rpx;
  font-size: 32rpx;
  color: #fff;
}
.head_info >.inforight{
  display: flex;
  align-items: center;
  width: 50%;
}
.head_info >.inforight >  .btnda{
  width: 50%;
}
.btndadis{
    width:155rpx;
  height: 60rpx;
  text-align: center;
  border-radius: 30px;
  line-height: 60rpx;
  background: #ccc;
  color: #fff;
  margin-left: 18rpx;
  font-size: 30rpx;
  margin-top: 15rpx;
  border: 1px solid #ccc;
}
.head_info >.inforight >  .btnda,.head_info >.inforight >  .btndet{
  width:155rpx;
  height: 60rpx;
  text-align: center;
  border-radius: 30px;
  line-height: 60rpx;
  background: #ff9035;
  color: #fff;
  margin-left: 18rpx;
  font-size: 30rpx;
  margin-top: 15rpx;
  border: 1px solid #fff;
}
.clock{
  height: 280rpx;
  padding: 15rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 6px;
}
.task{
  padding: 15rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 6px;
  margin-top: 30rpx;
}
.clock > .top > .clockData,task > .top > .clockData{
  font-size: 34rpx;
  color: #333333;
  margin-bottom: 5rpx;
}
.clock > .top > .clockInfo,.task > .top > .clockInfo{
  font-size: 30rpx;
  color: #999999;
}
.clock > .top{
  /* margin-bottom: 15rpx; */
}
.clock > .bottom{
  margin-top: 45rpx;
  display: flex;
    justify-content: space-between;
}
.clock > .bottom > .item > .imgs{
  width: 55rpx;
  height: 55rpx;
  border-radius: 50%;
  background: #e4e4e4;
  overflow: hidden;
   text-align: center;
   margin: 0 auto 5rpx;
}
.clock > .bottom > .item > .imgs image{
  width: 70%;
  display: block;
  margin: 12rpx 0 0 9rpx;
}
.clock > .bottom > .item > .font-number{
  font-size: 29rpx;
  color: #999;
  text-align: center;
}

.task > .bottom {
  display: flex;
  margin-top: 45rpx;
  align-items: center;
}
.task > .bottom > .imgs{
   width: 65rpx;
  height: 65rpx;
  border-radius: 50%;
  background: #97c73f;
  overflow: hidden;
  text-align: center;
}
.task > .bottom > .imgs image{
  width: 60%;
  display: block;
  margin: 15rpx 0 0 12rpx;
}
.task > .bottom >.info{
  flex: 1;
  margin-left: 25rpx;
}
.task > .bottom >.info > .font-task{
  font-size: 30rpx;
  color: #333;
}
.task > .bottom >.info > .font-info{
  color: #999999;
  font-size: 28rpx;
}
.task > .bottom >.btns{
  width: 140rpx;
  height: 50rpx;
  background: #fd9b12;
  text-align: center;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #fff;
  border-radius: 30rpx;
}

.task > .bottom >.disp{
  width: 140rpx;
  height: 50rpx;
  background: #ccc;
  text-align: center;
  line-height: 50rpx;
  font-size: 30rpx;
  color: #fff;
  border-radius: 30rpx;
}




